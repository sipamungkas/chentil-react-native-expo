{
  "expo": {
    "name": "chentil",
    "slug": "chentil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.sipamungkas",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#DF5188",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.sipamungkas.chentil",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/ic_launcher_foreground.png",
        "monchromeImage": "./assets/icon/ic_launcher_monochrome.png",
        "backgroundImage": "./assets/icon/ic_launcher_background.png",
        "backgroundColor": "#DF5188"
      }
    }
  }
}
