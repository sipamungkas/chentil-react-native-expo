{
  "expo": {
    "name": "Chentil",
    "slug": "chentil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/ic_launcher.png",
    "scheme": "com.sipamungkas",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.sipamungkas.chentil",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/ic_launcher_foreground.png",
        "monchromeImage": "./assets/icon/ic_launcher_monochrome.png",
        "backgroundImage": "./assets/icon/ic_launcher_background.png"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sipamungkas.chentil"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#241C1C",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
